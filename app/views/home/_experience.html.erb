<div class="container">
<% obj.each do |experience| %>
    <div class="row">
        <div class="col-xs-8 col-xs-offset-2">
            <div class="well well-lg">
                <div class="row">
                    <div class="col-lg-12">
                    
                       <h5> <%= experience.title %></h5>
                       <div class="col-lg">
                            <%= experience.work_period %>
                            <%= experience.description %>
                       </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% end %>